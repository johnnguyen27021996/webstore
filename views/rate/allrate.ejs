<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>All Product</title>
</head>

<body>
    <header>
        <nav>
            <ul>
                <li>PRODUCTS</li>
                <li><a href="">Members</a></li>
                <li><a href="/product">Products</a></li>
                <li><a href="/order">Orders</a></li>
                <li><a href="/rate">Rates</a></li>
                <li><a href="">Ships</a></li>
                <li><a href="">Payments</a></li>
                <li><a href="/admin/logout">Logout</a></li>
            </ul>
        </nav>
    </header>
    <section>
        <table border="1">
            <thead>
                <th>Customer Name</th>
                <th>Product Name</th>
                <th>Star</th>
                <th>Comment</th>
                <th>Time</th>
                <th>Action</th>
            </thead>
            <tbody>
                <% rates.forEach(item=>{ %>
                <tr>
                    <td><%= item.createBy %>
                    </td>
                    <td><%= item.populate_product.name %></td>
                    <td><%= item.star %></td>
                    <td><%= item.comment %></td>
                    <td><%= item.createAt %></td>
                    <td><a href="/rate/delete/<%= item._id %>">Delete</a></th>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </section>
    <section>
        <nav>
            <ul>
                <li>Pre</li>
                <% for(let i=0; i<pagination; i++){ %>
                <li><a href="/rate/p=<%= (i+1) %>"><%= (i+1) %></a></li>
                <% } %>
                <li>Next</li>
            </ul>
        </nav>
    </section>
</body>

</html>